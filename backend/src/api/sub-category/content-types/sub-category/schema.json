{
  "kind": "collectionType",
  "collectionName": "sub_categories",
  "info": {
    "singularName": "sub-category",
    "pluralName": "sub-categories",
    "displayName": "Sub Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "subcategory_id": {
      "type": "customField",
      "options": {
        "uuid-format": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
      },
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "parent_id": {
      "type": "customField",
      "options": {
        "uuid-format": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
      },
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "sub_categories"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "sub_categories"
    }
  }
}
